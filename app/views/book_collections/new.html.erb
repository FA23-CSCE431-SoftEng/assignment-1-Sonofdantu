<%= link_to("Back to List", book_collections_path, :class => 'back-link') %>

<div class="book_collections new">
  <h2>Create Book</h2>

  <%= form_for(@book) do |f| %>
    <!-- url: book_collections_path, method: :post -->
    
    <%= render(partial: 'form', locals: { f: f }) %>

    <div class="form-buttons">
      <%= f.submit("Create Book") %>
    </div>

  <% end %>
</div>
